<script>
	//Focus on first element on page
	$(window).load(function () {
		$(':input:not([readonly]):visible:enabled:first').focus();
	});
	//Make the default behavior of the enter key on inputs to trigger the next / continue button
	 $(document).keypress(function(event){
		if((event.keyCode ? event.keyCode : event.which) == '13'){
			if (event.target.tagName.toLowerCase() != "textarea" && (event.target.type == null || (event.target.type.toLowerCase() != "submit"))) {
				inputs = $("input:not([readonly],:hidden):visible:enabled, textarea:not([readonly]):visible:enabled, select:not([readonly]):visible:enabled");
				currentIndex = inputs.index($(event.target));
				if(inputs[currentIndex + 1] != null){
					event.preventDefault();
					inputs[currentIndex + 1].focus();
					if(inputs[currentIndex + 1].type == 'submit' && $(inputs[currentIndex + 1]).val() == 'Sign In'){
						$(inputs[currentIndex + 1]).click();
					}
				}
				return false;
			}
		}
		return true;
	});
</script>
